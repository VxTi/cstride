struct Test {
    some_field: float32;
}

const lambda: (int32) -> int32 = (x: int32): int32 -> {
    return x + 1;
};


module io {
    /** test */
    pub fn println(arg: string, ...): void
    {
        printf(arg, ...);
    }
}

let x = Test::{ some_field: 3.14 };

fn main(): void {
    io::println("z: %d", x.some_field);
    const result: int32 = lambda(10);

    io::println("result: %d", result);
}


